@unpublished{ruesch_lakesheds_2013,
	location = {Lake Geneva, {WI}},
	title = {Lakesheds and riverscapes: extending Wisconsin's hydro database with landscape attributes},
	url = {http://bit.ly/1edE2mY},
	abstract = {Most freshwater processes are closely coupled with landscape processes. For example, stream temperature is often dependent on riparian cover and stream flow is mostly dependent on spatiotemporal patterns of rainfall events. In order to characterize stream processes, we must first characterize the surrounding landscape. {GIS} tools have greatly improved our ability to automate this process; however the tools are only accessible to those with advanced programming skills and access to specialized software. Therefore, there is a need for a database of stream and lake attributes that is readily available for use by scientists, policymakers, and the general public. {EPA} recently released {NHDPlus} to fulfill this need, however this product has many limitations, including its scale (1:100,000), and the omission of lakes. Here, we present a database of high resolution (1:24,000 scale based on 10-meter digital elevation model) freshwater attributes for the state of Wisconsin. Over 160,000 streams and lakes were characterized with attributes such as land cover (The National Land Cover Dataset), climate ({PRISM} temperature and precipitation), and geology (e.g., bedrock geology and soil depth). For each attribute, we characterized each freshwater feature across 5 dimensions: the stream/lake level, the immediate riparian area, the cumulative upstream riparian area, the immediate watershed, and the cumulative upstream watershed. We also included attribution for feature connectivity, such as the distance to the nearest lake or large river. The scale of the delineation of each level is a substantial improvement on {NHDPlus.} Within the state of Wisconsin, the average {NHDPlus} watershed size is 2.9 sq. km. ({SD} = 5.6 sq. km.), whereas the average watershed in this database is 0.9 sq. km. ({SD} = 3.1 sq. km.). {NHDPlus} watersheds were delineated based on stream features drawn from 1:100,000-scale digitized maps using a 30-meter {DEM}, whereas watersheds in this database were delineated based on 1:24,000-scale digitized maps using a 10-meter {DEM.} Access to a database with this level of detail will greatly increase the quality and efficiency at which scientists and managers can answer questions and formulate policy for freshwater issues that are dependent on landscape drivers.},
	note = {{WLIA} Annual Conference},
	author = {\textbf{Ruesch, A. S.} and Diebel, M. W. and Menuz, D. R.},
	date = {2013-02}
}

