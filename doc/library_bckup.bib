@article{mellina_stream_2002,
	title = {Stream temperature responses to clearcut logging in British Columbia: the moderating influences of groundwater and headwater lakes},
	volume = {59},
	issn = {0706-{652X}},
	doi = {Article},
	shorttitle = {Stream temperature responses to clearcut logging in British Columbia: the moderating influences of groundwater and headwater lakes},
	abstract = {Although the future timber supply in the northern hemisphere is expected to come from boreal and subboreal forests, little research has been conducted in these regions that examines the temperature responses of small, lake-headed streams to streamside timber harvesting. We examined the temperature patterns of two subboreal outlet streams in north-central British Columbia for I year before and 3 years after clearcut logging and found only modest changes (averaging 0.05-{1.1degreesC)} with respect to summer daily maximum and minimum temperatures, diurnal fluctuations, and stream cooling. A multistream comparative survey conducted in the same geographic region revealed that streams headed by small lakes or swamps tended to cool as they flowed downstream, and headwater streams warmed, regardless of whether or not timber harvesting took place. Stream cooling was attributed to a combination of warm outlet temperatures (promoted by the presence of the lakes) and cold groundwater inflows. A regression model revealed that summertime downstream warming or cooling in headwater and outlet streams could be predicted by upstream maximum summer temperatures and canopy cover. Lentic water bodies and groundwater inflows are important determinants of stream temperature patterns in subboreal forests and may subsequently moderate their responses to streamside harvesting.},
	pages = {1886-1900},
	journaltitle = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Mellina, E. and Moore, R. D. and Hinch, S. G. and Macdonald, J. S. and Pearson, G.},
	date = {2002},
