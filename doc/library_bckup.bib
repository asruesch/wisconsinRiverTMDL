	keywords = {{NORTHERN} {SQUAWFISH} {PTYCHOCHEILUS} {STIZOSTEDION-VITREUM-VITREUM} {FAST-START} {PERFORMANCE} {GAS} {SUPERSATURATION} {SIZE} {PREY} {STEELHEAD} {OREGONENSIS} {CONSUMPTION} {ABUNDANCE}}
