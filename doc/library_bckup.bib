@article{beechie_approach_1999,
	title = {An approach to restoring salmonid habitat-forming processes in Pacific Northwest watersheds},
	volume = {24},
	issn = {0363-2415},
	url = {://WOS:000079445100002},
	doi = {10.1577/1548-8446(1999)024<0006:aatrsh>2.0.co;2},
	shorttitle = {An approach to restoring salmonid habitat-forming processes in Pacific Northwest watersheds},
	abstract = {We present an approach to diagnosing salmonid habitat degradation and restoring habitat-forming processes that is focused on causes of habitat degradation rather than on effects of degradation. The approach is based on the understanding that salmonid Stocks are adapted to local freshwater conditions and that their environments are naturally temporally dynamic. In this context, we define a goal of restoring the natural rates and magnitudes of habitat-forming processes, and we allow for locally defined restoration priorities. The goal requires that historical reconstruction focus on diagnosing disruptions to processes rather than conditions. Historical reconstruction defines the suite of restoration tasks, which then may be prioritized based on local biological objectives, We illustrate the use of this approach for two habitat-forming processes: sediment supply and stream shading. We also briefly contrast this approach to several others that maybe used as components of a restoration strategy.},
	pages = {6-15},
